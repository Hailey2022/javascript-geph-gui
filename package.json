{
  "name": "gephgui",
  "version": "0.0.1",
  "private": true,
  "homepage": ".",
  "main": "public/electron.js",
  "dependencies": {
    "electron-is-dev": "^1.1.0"
  },
  "scripts": {
    "electron:start": "electron .",
    "electron:pack": "react-scripts build; electron-builder -wl --dir",
    "electron:dist": "react-scripts build; electron-builder",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron-pack": "build --em.main=build/electron.js"
  },
  "build": {
    "appId": "io.geph.geghgui",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    },
    "linux": {
      "target": [
        "AppImage"
      ]
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "description": "An Ionic project",
  "devDependencies": {
    "electron": "^6.0.10",
    "electron-builder": "^21.2.0",
    "electron-packager": "^14.0.6",
    "@ionic/react": "^4.9.0-rc.2",
    "@ionic/react-router": "^4.9.0-rc.2",
    "@types/jest": "24.0.18",
    "@types/node": "12.7.5",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "@types/react-router": "^5.0.3",
    "@types/react-router-dom": "^4.3.1",
    "axios": "^0.19.0",
    "datejs": "^1.0.0-rc3",
    "flag-icon-css": "^3.4.0",
    "ionicons": "^4.6.3",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-helmet": "^5.2.1",
    "react-odometerjs": "^2.1.1",
    "react-redux": "^7.1.1",
    "react-router": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "react-scripts": "3.1.2",
    "react-world-flags": "^1.3.0",
    "redux": "^4.0.4",
    "typescript": "3.6.3"
  }
}
